   29 $@6: %empty

   30 case_statement: _CASE literal _COLON $@6 statement break_statement

   31 break_statement: %empty
   32                | _BREAK _SEMICOLON

   33 default_statement: %empty

   34 $@7: %empty

   35 default_statement: _DEFAULT _COLON $@7 statement

   36 compound_statement: _LBRACKET statement_list _RBRACKET

   37 assignment_statement: _ID _ASSIGN num_exp _SEMICOLON

   38 num_exp: exp
   39        | num_exp _AROP exp

   40 exp: literal
   41    | _ID
   42    | function_call
   43    | _LPAREN num_exp _RPAREN

   44 literal: _INT_NUMBER
   45        | _UINT_NUMBER

   46 $@8: %empty

   47 function_call: _ID $@8 _LPAREN argument _RPAREN

   48 argument: %empty
   49         | num_exp

   50 if_statement: if_part
   51             | if_part _ELSE statement

   52 @9: %empty

   53 $@10: %empty

   54 if_part: _IF _LPAREN @9 rel_exp $@10 _RPAREN statement

   55 rel_exp: num_exp _RELOP num_exp

   56 return_statement: _RETURN num_exp _SEMICOLON


