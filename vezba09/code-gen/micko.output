   20          | if_statement
   21          | return_statement
   22          | switch_statement

   23 $@3: %empty

   24 @4: %empty

   25 $@5: %empty

   26 switch_statement: _SWITCH _LPAREN _ID $@3 _RPAREN _LBRACKET @4 case_statements $@5 default_statement _RBRACKET

   27 case_statements: case_statement
